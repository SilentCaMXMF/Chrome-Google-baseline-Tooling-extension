{
  "name": "baseline-checker",
  "version": "1.0.0",
  "description": "Check web features against Baseline data directly in your Cloud IDE",
  "scripts": {
    "build": "npm run clean && npm run copy-assets",
    "clean": "rimraf dist/*",
    "copy-assets": "cpx \"**/*.{html,css,js,json,png,svg}\" dist --exclude=node_modules dist",
    "dev": "npm run build -- --watch",
    "generate-icons": "node scripts/generate-icons.js",
    "lint": "eslint . --ext .js",
    "test": "jest",
    "package": "npm run build && bestzip baseline-checker-dist.zip dist/*"
  },
  "keywords": [
    "chrome-extension",
    "web-development",
    "baseline",
    "compatibility",
    "ide"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "bestzip": "^2.2.1",
    "canvas": "^3.2.0",
    "cpx": "^1.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.8.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
